<!DOCTYPE html>
<html>
<head>
    <title>Search Page</title>
    <script src="https://cdn.adblockplus.org/adblockpluschrome-1.0.4.js"></script>
    <script>
        function searchGoogle() {
            var query = document.getElementById('searchQuery').value;
            if (query.trim() !== '') {
                var url = 'https://www.google.com/search?q=' + encodeURIComponent(query);
                var newWindow = window.open(url, '_blank');
                if (newWindow) {
                    newWindow.addEventListener('DOMContentLoaded', function () {
                        var adblockplus = new newWindow.AdblockPlus();
                        adblockplus.init().then(function () {
                            adblockplus.enableBlocking();
                        });
                    });
                }
            }
        }
    </script>
</head>
<body>
    <h1>Search Page</h1>
    <form onsubmit="searchGoogle(); return false;">
        <input type="text" id="searchQuery" placeholder="Enter your search query">
        <input type="submit" value="Search">
    </form>
</body>
</html>
